{
    "extends": ["next/core-web-vitals", "plugin:prettier/recommended"],
    "plugins": ["prettier"],
    "rules": {
        "prettier/prettier": [
            "error",
            {
                "semi": true,
                "singleQuote": true,
                "printWidth": 80,
                "tabWidth": 4,
                "trailingComma": "es5"
            }
        ]
    }
}
